apiVersion: v1
kind: Secret
type: Opaque
metadata:
    name: thanos-objstore-secret
    namespace: monitoring
stringData:
  objstore.yml: |-
    type: s3
    config:
      bucket: thanos
      endpoint: truenas.${SECRET_DOMAIN}:9000
      region: us-east
      access_key: ${SECRET_S3_ACCESS_KEY}
      secret_key: ${SECRET_S3_SECRET_KEY}
      insecure: true
