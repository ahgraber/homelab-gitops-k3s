{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    "docker:enableMajor",
    "helpers:pinGitHubActionDigestsToSemver",
    ":automergePr", // ":automergeBranch",
    ":configMigration",
    ":dependencyDashboard",
    ":disableRateLimiting",
    ":docker",
    ":enablePreCommit",
    // ":rebaseStalePrs",
    ":renovatePrefix",
    ":semanticCommits",
    ":separateMultipleMajorReleases",
    ":separateMultipleMinorReleases",
    ":separatePatchReleases",
    ":timezone(America/New_York)",
    "github>ahgraber/homelab-gitops-k3s//.renovate/allowedVersions.json5",
    "github>ahgraber/homelab-gitops-k3s//.renovate/autoMerge.json5",
    "github>ahgraber/homelab-gitops-k3s//.renovate/customManagers.json5",
    "github>ahgraber/homelab-gitops-k3s//.renovate/grafanaDashboards.json5",
    "github>ahgraber/homelab-gitops-k3s//.renovate/groups.json5",
    "github>ahgraber/homelab-gitops-k3s//.renovate/labels.json5",
    "github>ahgraber/homelab-gitops-k3s//.renovate/packageRules.json5",
    "github>ahgraber/homelab-gitops-k3s//.renovate/semanticCommits.json5",
  ],
  dependencyDashboardTitle: "Renovate Dashboard ðŸ¤–",
  suppressNotifications: [
    "prEditedNotification",
    "prIgnoreNotification"
  ],
  minimumReleaseAge: "3 days",
  ignorePaths: [
    "**/*.sops.*",
    "**/.archive/**",
    "**/resources/**",
    ".copier-answers.yaml"
  ],
  flux: {
    managerFilePatterns: ["/(^|/)kubernetes/.+\\.ya?ml(\\.j2)?$/"],
  },
  "helm-values": {
    managerFilePatterns: ["/(^|/)kubernetes/.+\\.ya?ml(\\.j2)?$/"],
  },
  kubernetes: {
    managerFilePatterns: [
      "/(^|/)\\.taskfiles/.+\\.ya?ml(\\.j2)?$/",
      "/(^|/)ansible/.+\\.ya?ml(\\.j2)?$/",
      "/(^|/)kubernetes/.+\\.ya?ml(\\.j2)?$/",
    ],
  },
}
